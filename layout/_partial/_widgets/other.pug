section.widget
  h3.widget-title
    | #{__('widget.other')}
  ul.widget-list
    li
      a(href=url_for(config.feed.path)) #{__('widget.feed')}
    each link, key in theme.widgets.other.links
      li
        a(href=url_for(link) target='_blank') #{__(key)}
    li
      a#back-to-top(href='#') #{__('widget.backtotop')}
